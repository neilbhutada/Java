    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        Double d = Double.parseDouble(jTextField2.getText());
       Double e = Double.parseDouble(jTextField1.getText());
       Double f ;
       if (d==0)
       {
          f= e-d;
          jTextField1.setText("");
        
       jTextField2.setText(""+f);
       
       }
       else
       {
           f = d-e;
           jTextField1.setText("");
            

       jTextField2.setText(""+f);
       
      
       }
      
       
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
Double la = Double.parseDouble(jTextField2.getText());
if(la==0)
{jTextField2.setText("1");
        
}

else
{ 
    
}
Double g = Double.parseDouble(jTextField2.getText());
       Double h = Double.parseDouble(jTextField1.getText());
       Double i = g*h;
       
         jTextField1.setText("");
      
       jTextField2.setText(""+i);
       
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
Double cha = Double.parseDouble(jTextField2.getText());
if(cha==0)
{
    jTextField2.setText("1");
    
}
else 
{ 
    
}

        
         
           float j = Float.parseFloat(jTextField2.getText());
       float k = Float.parseFloat(jTextField1.getText());
       float l = k/j;
       jTextField1.setText("");
       
       jTextField2.setText(""+l);
       
       
       
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       jTextField1.setText("");
       jTextField2.setText("0");
       
    }                                        


