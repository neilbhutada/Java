    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       Double a = Double.parseDouble(jTextField2.getText());
       Double b = Double.parseDouble(jTextField1.getText());
       Double c = a+b;
       jTextField1.setText("");
      
       jTextField2.setText(""+c);
       
     
       
       
    }                                        

